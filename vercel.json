{
  "version": 2,
  "builds": [
    {
      "src": "peach-web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [ 
    { 
      "src": "/api/(.*)", 
      "dest": "/api/$1" 
    },
    {
      "src": "/(.*)",
      "dest": "/peach-web/$1"
    }
  ]
}
