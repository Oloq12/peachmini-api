{
  "version": 2,
  "builds": [
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "peach-web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "" },
        { "key": "Content-Security-Policy", "value": "frame-ancestors https://web.telegram.org https://*.telegram.org https://t.me https://*.t.me;" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/test",
      "destination": "/api/test.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/health",
      "destination": "/api/index.js"
    },
    {
      "source": "/girls",
      "destination": "/api/index.js"
    },
    {
      "source": "/girls/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/chat/reply",
      "destination": "/api/index.js"
    },
    {
      "source": "/ref/status",
      "destination": "/api/index.js"
    },
    {
      "source": "/ref/apply",
      "destination": "/api/index.js"
    },
    {
      "source": "/quests/checkin",
      "destination": "/api/index.js"
    },
    {
      "source": "/quests/status",
      "destination": "/api/index.js"
    },
    {
      "source": "/quests/complete",
      "destination": "/api/index.js"
    },
    {
      "source": "/payments/createInvoice",
      "destination": "/api/index.js"
    },
    {
      "source": "/chats",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/peach-web/$1"
    }
  ]
}
